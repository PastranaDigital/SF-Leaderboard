<aura:component>
	<lightning:workspaceAPI aura:id="workspace" />
	<aura:attribute name="listId" type="String" access="public" />
	<aura:attribute name="selectedIdsList" type="Id[]" access="public" />
	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />

	<c:massActionAthleteLwc 
		selectedIdsList="{!v.selectedIdsList}"
		listId="{!v.listId}"
		onshowtoast="{!c.handleToastEvent}"
		onnavigatetolistview="{!c.handleNavigateToListviewEvent}"
		onnavigatetorecord="{!c.handleNavigateToRecord}" />
</aura:component>	
